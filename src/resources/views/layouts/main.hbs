<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MagicPost</title>
    <link rel="stylesheet" href="/css/home.css">
    <link rel="stylesheet" href="/css/app.css">
</head>
<script src="/jquery/jquery.js"></script>
<script src='/js/Cookies.js'></script>
<body>

    {{#if transaction_staff_header}}
        {{> transaction_staff_header/header}}
    {{else if warehouse_staff_header}} 
        {{> warehouse_staff_header/header}}
    {{else if warehouse_manager_header}} 
        {{> warehouse_manager_header/header}}
    {{else if transaction_manager_header}} 
        {{> transaction_manager_header/header}}
    {{else if manager_header}} 
        {{> manager_header/header}}
    {{else if (or none_header notFound) }} 
        
    {{else}}
        {{> header}}
    {{/if}}


    {{{body}}}


    {{#if (or3 none_header noFooter notFound)}}
    
    {{else}}
        {{> footer}}
    {{/if}}
    
</body>
</html>